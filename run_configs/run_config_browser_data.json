{
    "data_config": {
        "data_source": "data/real/browserversiondf.csv",
        "sampling_period": "1d",
        "timestamp_name": "Date"
    },
    "metric_config": {
        "metric_name": "Visits",
        "model": "poisson",
        "dtype_kind": "u",
        "augmentation_config": {
            "mode": "constant",
            "value": 15000
        }
    },
    "gloria_config": {
        "n_changepoints": 25,
        "changepoint_range": 0.8,
        "seasonality_mode": "additive",
        "seasonality_prior_scale": 50.0,
        "changepoint_prior_scale": 0.05,
        "interval_width": 0.8,
        "uncertainty_samples": 1000,
        "optimize_mode": "MLE",
        "sample": false
    }
}